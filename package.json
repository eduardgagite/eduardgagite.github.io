{
  "name": "eduardgagite.github.io",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node scripts/generate-materials-json.mjs && node scripts/generate-sitemap.mjs && vite",
    "build": "node scripts/generate-materials-json.mjs && node scripts/generate-sitemap.mjs && tsc -b && vite build",
    "preview": "vite preview --port 5173 --strictPort",
    "typecheck": "tsc -b --pretty false",
    "lint": "npm run typecheck",
    "test": "node --test scripts/tests/*.test.mjs",
    "check": "npm run lint && npm run test"
  },
  "dependencies": {
    "framer-motion": "^12.23.24",
    "gray-matter": "^4.0.3",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.5",
    "prism-react-renderer": "^2.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.2",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.27.0",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  },
  "engines": {
    "node": ">=18"
  }
}
